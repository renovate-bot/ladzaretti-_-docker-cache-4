{
  "name": "docker-cache",
  "version": "0.3.0",
  "description": "Cache Docker Images Whether Built or Pulled",
  "homepage": "https://github.com/ScribeMD/docker-cache#readme",
  "main": "dist/main/index.js",
  "repository": "github:ScribeMD/docker-cache",
  "author": "Kurt von Laven <kurt.von.laven@gmail.com>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "yarn run build:main && yarn run build:post",
    "build:main": "yarn run ncc build src/main.ts --out dist/main --minify",
    "build:post": "yarn run ncc build src/post.ts --out dist/post --minify",
    "test": "yarn run tsc && yarn node --experimental-vm-modules \"$(yarn bin jest)\""
  },
  "engines": {
    "node": "18.20.4",
    "yarn": "3.8.6"
  },
  "packageManager": "yarn@3.8.6",
  "dependencies": {
    "@actions/cache": "3.2.4",
    "@actions/core": "1.11.1"
  },
  "devDependencies": {
    "@fast-check/jest": "1.8.2",
    "@jest/globals": "29.7.0",
    "@jest/types": "29.6.3",
    "@tsconfig/node18-strictest-esm": "1.0.1",
    "@types/eslint": "8.56.12",
    "@types/jest": "29.5.14",
    "@types/node": "18.19.59",
    "@types/prettier": "2.7.3",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "@vercel/ncc": "0.38.2",
    "@yarnpkg/sdks": "2.7.4",
    "eslint": "8.57.1",
    "eslint-config-prettier": "8.10.0",
    "fast-check": "3.22.0",
    "jest": "29.7.0",
    "jest-junit": "15.0.0",
    "jest-mock": "29.7.0",
    "prettier": "2.8.8",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "typescript": "4.9.5"
  }
}
